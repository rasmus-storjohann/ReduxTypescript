{
  "name": "pathways-frontend",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.2.2",
    "@types/react": "^16.0.40",
    "@types/react-native": "^0.52.20",
    "@types/react-redux": "^5.0.15",
    "@types/react-test-renderer": "^16.0.1",
    "babel-jest": "^22.4.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "jest": "^22.4.3",
    "jest-expo": "25.0.0",
    "react-dom": "^16.3.1",
    "react-native-scripts": "1.11.1",
    "react-test-renderer": "^16.3.1",
    "redux-devtools": "^3.4.1",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "tslint-immutable": "^3.4.2",
    "typescript": "^2.7.2"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "postinstall": "bin/postinstall.sh",
    "lint": "tslint --rules-dir node_modules/tslint-immutable/rules/ src/**/*.ts",
    "tsc": "tsc",
    "jest": "jest",
    "clean": "rimraf lib/**/*.js lib/**/*.js.map",
    "build": "yarn run clean && yarn run tsc",
    "test": "yarn run build && yarn run jest",
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios"
  },
  "jest": {
    "preset": "react-native",
    "setupTestFrameworkScriptFile": "<rootDir>/lib/application/__tests__/setup_tests.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "<rootDir>/lib/**/*.test.js"
    ]
  },
  "dependencies": {
    "expo": "^25.0.0",
    "react": "16.2.0",
    "react-native": "0.52.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-persist": "^5.9.1"
  }
}