  "jest": {
    "preset": "react-native",
    "testMatch": [
      "<rootDir>/artifacts/**/tests/test*.js"
    ]
  },
  "dependencies": {
    "@types/jest": "^22.2.2",
    "@types/react": "^16.0.40",
    "@types/react-native": "^0.52.19",
    "concurrently": "^3.5.1",
    "expo": "^25.0.0",
    "react": "16.2.0",
    "react-native": "0.52.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  }
